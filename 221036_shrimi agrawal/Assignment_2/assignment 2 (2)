    "import numpy as np\n",
    "arr=np.array([1,2,5,3])\n",
    "arr1=np.array([4,6,8,2])\n",
    "sum = 0\n",
    "multiplication = 1\n",
    "for i in arr:\n",
    "    sum = sum+i\n",
    "    \n",
    "print(sum) #addition of elements in array\n",
